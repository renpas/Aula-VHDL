#VRML V2.0 utf8
# Programa shapes.wrl
# Demonstra o uso de multiplos shapes no mesmo arquivo

Transform
{
	translation 2 0 0
	children [
		Shape
		{
			appearance Appearance
			{ material Material { } }
			geometry Box { size 1.0 1.0 1.0 }
		}
	]
}

Transform
{
	translation 0 0 0
	children [
		Shape
		{
			appearance Appearance
			{ material Material { diffuseColor 1 1 0 } }
			geometry Sphere { radius 0.7 }
		}
	]
}

